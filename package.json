{
  "name": "@craigmiller160/udemy-crypto",
  "version": "0.0.1-beta",
  "scripts": {
    "forceBuild": "tsc",
    "build": "yarn validate && tsc",
    "start:dev": "env-cmd -f .env.primary nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node src'",
    "start:primary": "env-cmd -f .env.primary ts-node src",
    "start:secondary": "env-cmd -f .env.secondary ts-node src",
    "start:tertiary": "env-cmd -f .env.tertiary ts-node src",
    "lint": "eslint . --fix --max-warnings=0",
    "test": "jest",
    "validate": "yarn lint && yarn test",
    "prepare": "husky install"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.1.6",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "fp-ts": "^2.11.5",
    "https": "^1.0.0",
    "io-ts": "^2.2.16",
    "nanoid": "^3.1.30",
    "nocache": "^3.0.1",
    "pino": "^7.1.0",
    "pino-pretty": "^7.2.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^1.1.1",
    "@types/crypto-js": "^4.0.2",
    "@types/elliptic": "^6.4.14",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
